{
  "name": "ndthanhdev.github.io",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "deploy": "gatsby build && gh-pages -d public -b master",
    "prettify": "prettier --check **/*.{json,html,md}",
    "prettify:fix": "prettier --write **/*.{json,html,md}",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lint:fix": "tslint -p tsconfig.json -c tslint.json --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ndthanhdev/ndthanhdev.github.io.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ndthanhdev/ndthanhdev.github.io/issues"
  },
  "homepage": "https://github.com/ndthanhdev/ndthanhdev.github.io#readme",
  "dependencies": {
    "gatsby": "^2.3.25",
    "gatsby-image": "^2.0.39",
    "gatsby-plugin-manifest": "^2.0.29",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sharp": "^2.0.35",
    "gatsby-plugin-typescript": "^2.0.12",
    "gatsby-remark-copy-linked-files": "^2.0.11",
    "gatsby-source-filesystem": "^2.0.29",
    "gatsby-transformer-remark": "^5.25.1",
    "gatsby-transformer-sharp": "^2.1.18",
    "gh-pages": "^2.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "typescript": "^3.4.4"
  },
  "devDependencies": {
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.8",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "tslint": "^5.16.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{json,html,md}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{ts,tsx}": [
      "npm run lint:fix",
      "prettier --write",
      "git add"
    ]
  }
}
